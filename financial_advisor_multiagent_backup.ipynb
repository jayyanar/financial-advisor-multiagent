    "def validate_and_sanitize_ticker(ticker: str) -> str:\n",
    "    \"\"\"\n",
    "    Enhanced ticker symbol validation and sanitization.\n",
    "    \n",
    "    Args:\n",
    "        ticker: Raw ticker input from user\n",
    "    \n",
    "    Returns:\n",
    "        Sanitized ticker symbol\n",
    "    \n",
    "    Raises:\n",
    "        ValidationError: If ticker is invalid\n",
    "    \"\"\"\n",
    "    if not ticker or not isinstance(ticker, str):\n",
    "        raise ValidationError(\"Ticker symbol must be a non-empty string\")\n",
    "    \n",
    "    # Sanitize input\n",
    "    ticker = ticker.strip().upper()\n",
    "    \n",
    "    # Remove any non-alphabetic characters\n",
    "    ticker = re.sub(r'[^A-Z]', '', ticker)\n",
    "    \n",
    "    # Validate format\n",
    "    if len(ticker) < 1:\n",
    "        raise ValidationError(\"Ticker symbol cannot be empty after sanitization\")\n",
    "    \n",
    "    if len(ticker) > 5:\n",
    "        raise ValidationError(f\"Ticker symbol '{ticker}' is too long (max 5 characters)\")\n",
    "    \n",
    "    # Check for common invalid patterns\n",
    "    invalid_patterns = ['TEST', 'NULL', 'NONE', 'XXXX']\n",
    "    if ticker in invalid_patterns:\n",
    "        raise ValidationError(f\"'{ticker}' is not a valid ticker symbol\")\n",
    "    \n",
    "    return ticker\n",
    "\n",
    "def validate_risk_tolerance(risk: str) -> str:\n",
    "    \"\"\"\n",
    "    Enhanced risk tolerance validation.\n",
    "    \n",
    "    Args:\n",
    "        risk: Raw risk tolerance input\n",
    "    \n",
    "    Returns:\n",
    "        Standardized risk tolerance\n",
    "    \n",
    "    Raises:\n",
    "        ValidationError: If risk tolerance is invalid\n",
    "    \"\"\"\n",
    "    if not risk or not isinstance(risk, str):\n",
    "        raise ValidationError(\"Risk tolerance must be specified\")\n",
    "    \n",
    "    risk = risk.strip().lower()\n",
    "    \n",
    "    # Map variations to standard values\n",
    "    risk_mapping = {\n",
    "        'conservative': 'Conservative',\n",
    "        'cons': 'Conservative',\n",
    "        'low': 'Conservative',\n",
    "        'safe': 'Conservative',\n",
    "        'moderate': 'Moderate',\n",
    "        'mod': 'Moderate',\n",
    "        'medium': 'Moderate',\n",
    "        'balanced': 'Moderate',\n",
    "        'aggressive': 'Aggressive',\n",
    "        'agg': 'Aggressive',\n",
    "        'high': 'Aggressive',\n",
    "        'risky': 'Aggressive'\n",
    "    }\n",
    "    \n",
    "    if risk not in risk_mapping:\n",
    "        valid_options = list(set(risk_mapping.values()))\n",
    "        raise ValidationError(f\"Invalid risk tolerance '{risk}'. Valid options: {', '.join(valid_options)}\")\n",
    "    \n",
    "    return risk_mapping[risk]\n",
    "\n",
