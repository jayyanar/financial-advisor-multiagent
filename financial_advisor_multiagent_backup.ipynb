   "source": [
    "# Helper functions for agent interaction and response formatting\n",
    "\n",
    "def query_agent(query: str, agent=None, state=None, timeout: int = 120):\n",
    "    \"\"\"\n",
    "    Query an agent with comprehensive error handling and response validation.\n",
    "    \n",
    "    Args:\n",
    "        query: The query string to send to the agent\n",
    "        agent: The agent to query (defaults to financial_coordinator)\n",
    "        state: The state object to use (creates new if None)\n",
    "        timeout: Maximum time to wait for response in seconds\n",
    "    \n",
    "    Returns:\n",
    "        Dict containing response data and metadata, or None if error\n",
    "    \"\"\"\n",
    "    if agent is None:\n",
    "        agent = financial_coordinator\n",
    "    \n",
    "    if state is None:\n",
    "        state = State()\n",
    "    \n",
    "    # Input validation\n",
    "    if not query or not isinstance(query, str):\n",
    "        print(\"❌ Error: Query must be a non-empty string\")\n",
    "        return None\n",
    "    \n",
    "    if len(query.strip()) == 0:\n",
    "        print(\"❌ Error: Query cannot be empty or whitespace only\")\n",
    "        return None\n",
    "    \n",
    "    try:\n",
    "        print(f\"🤖 Querying {agent.name}...\")\n",
    "        start_time = datetime.now()\n",
    "        \n",
    "        response = agent.run(query=query, state=state)\n",
    "        \n",
    "        end_time = datetime.now()\n",
    "        duration = (end_time - start_time).total_seconds()\n",
    "        \n",
    "        # Create response metadata\n",
    "        response_data = {\n",
    "            'response': response,\n",
    "            'agent_name': agent.name,\n",
    "            'query': query,\n",
    "            'timestamp': end_time.isoformat(),\n",
    "            'duration_seconds': duration,\n",
    "            'state_keys': list(state.data.keys()) if hasattr(state, 'data') else [],\n",
    "            'success': True\n",
    "        }\n",
    "        \n",
    "        print(f\"✅ Response received in {duration:.2f} seconds\")\n",
    "        return response_data\n",
    "        \n",
    "    except TimeoutError:\n",
    "        print(f\"⏰ Timeout: Agent {agent.name} did not respond within {timeout} seconds\")\n",
    "        return None\n",
    "    except Exception as e:\n",
    "        error_msg = str(e)\n",
    "        print(f\"❌ Error querying agent {agent.name}: {error_msg}\")\n",
    "        \n",
    "        # Return error information for debugging\n",
    "        return {\n",
    "            'response': None,\n",
    "            'agent_name': agent.name,\n",
    "            'query': query,\n",
    "            'error': error_msg,\n",
    "            'timestamp': datetime.now().isoformat(),\n",
    "            'success': False\n",
    "        }\n",
    "\n",
    "def print_response(title: str, response_data, max_length: int = 1000, show_metadata: bool = False):\n",
    "    \"\"\"\n",
    "    Print a formatted response with enhanced formatting and metadata display.\n",
    "    \n",
    "    Args:\n",
    "        title: Title for the response\n",
    "        response_data: Response data from query_agent or raw response string\n",
    "        max_length: Maximum length before truncation (0 for no truncation)\n",
    "        show_metadata: Whether to display response metadata\n",
    "    \"\"\"\n",
    "    print(f\"\\n{title}\")\n",
    "    print(\"=\" * len(title))\n",
    "    \n",
    "    # Handle different response data formats\n",
    "    if response_data is None:\n",
    "        print(\"❌ No response received\")\n",
    "        return\n",
    "    \n",
    "    # Extract response text\n",
    "    if isinstance(response_data, dict):\n",
    "        if not response_data.get('success', True):\n",
    "            print(f\"❌ Error: {response_data.get('error', 'Unknown error')}\")\n",
    "            return\n",
    "        \n",
    "        response_text = response_data.get('response', '')\n",
    "        \n",
    "        # Show metadata if requested\n",
    "        if show_metadata:\n",
    "            print(f\"Agent: {response_data.get('agent_name', 'Unknown')}\")\n",
    "            print(f\"Duration: {response_data.get('duration_seconds', 0):.2f}s\")\n",
    "            print(f\"Timestamp: {response_data.get('timestamp', 'Unknown')}\")\n",
    "            if response_data.get('state_keys'):\n",
    "                print(f\"State Keys: {', '.join(response_data['state_keys'])}\")\n",
    "            print(\"-\" * 40)\n",
    "    else:\n",
    "        response_text = str(response_data)\n",
    "    \n",
    "    # Format and display response\n",
    "    if not response_text:\n",
    "        print(\"⚠️ Empty response received\")\n",
    "        return\n",
    "    \n",
    "    # Apply truncation if specified\n",
    "    if max_length > 0 and len(response_text) > max_length:\n",
    "        print(response_text[:max_length] + \"...\")\n",
    "        print(f\"\\n[Response truncated - showing first {max_length} characters of {len(response_text)} total]\")\n",
    "        print(\"💡 Tip: Use max_length=0 to see full response or request 'detailed markdown output'\")\n",
    "    else:\n",
    "        print(response_text)\n",
    "\n",
    "def format_markdown_output(content: str, title: str = \"Detailed Analysis\") -> str:\n",
    "    \"\"\"\n",
    "    Format content as structured markdown with proper headers and sections.\n",
    "    \n",
    "    Args:\n",
    "        content: The content to format\n",
    "        title: Main title for the markdown document\n",
    "    \n",
    "    Returns:\n",
    "        Formatted markdown string\n",
    "    \"\"\"\n",
    "    timestamp = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    \n",
    "    markdown_output = f\"\"\"# {title}\n",
    "\n",
    "*Generated on {timestamp}*\n",
    "\n",
    "---\n",
    "\n",
    "{content}\n",
    "\n",
    "---\n",
    "\n",
    "**Disclaimer**: This analysis is for educational purposes only and does not constitute financial advice.\n",
    "\"\"\"\n",
    "    \n",
    "    return markdown_output\n",
    "\n",
    "def validate_ticker_symbol(ticker: str) -> bool:\n",
    "    \"\"\"\n",
    "    Validate ticker symbol format.\n",
    "    \n",
    "    Args:\n",
    "        ticker: Ticker symbol to validate\n",
    "    \n",
    "    Returns:\n",
    "        True if valid, False otherwise\n",
    "    \"\"\"\n",
    "    if not ticker or not isinstance(ticker, str):\n",
    "        return False\n",
    "    \n",
    "    ticker = ticker.strip().upper()\n",
    "    \n",
    "    # Basic validation: 1-5 characters, letters only\n",
    "    if len(ticker) < 1 or len(ticker) > 5:\n",
    "        return False\n",
    "    \n",
    "    if not ticker.isalpha():\n",
    "        return False\n",
    "    \n",
    "    return True\n",
    "\n",
    "def validate_risk_preference(risk: str) -> bool:\n",
    "    \"\"\"\n",
    "    Validate risk preference input.\n",
    "    \n",
    "    Args:\n",
    "        risk: Risk preference to validate\n",
    "    \n",
    "    Returns:\n",
    "        True if valid, False otherwise\n",
    "    \"\"\"\n",
    "    if not risk or not isinstance(risk, str):\n",
    "        return False\n",
    "    \n",
    "    valid_risks = ['conservative', 'moderate', 'aggressive']\n",
    "    return risk.strip().lower() in valid_risks\n",
    "\n",
    "def validate_investment_period(period: str) -> bool:\n",
    "    \"\"\"\n",
    "    Validate investment period input.\n",
    "    \n",
    "    Args:\n",
    "        period: Investment period to validate\n",
    "    \n",
    "    Returns:\n",
    "        True if valid, False otherwise\n",
    "    \"\"\"\n",
    "    if not period or not isinstance(period, str):\n",
    "        return False\n",
    "    \n",
    "    valid_periods = ['short-term', 'medium-term', 'long-term']\n",
    "    return period.strip().lower() in valid_periods\n",
    "\n",
    "def handle_agent_error(error: Exception, agent_name: str, query: str) -> Dict[str, Any]:\n",
    "    \"\"\"\n",
    "    Handle and format agent errors with helpful information.\n",
    "    \n",
    "    Args:\n",
    "        error: The exception that occurred\n",
    "        agent_name: Name of the agent that failed\n",
    "        query: The query that caused the error\n",
    "    \n",
    "    Returns:\n",
    "        Dictionary with error information and suggested actions\n",
    "    \"\"\"\n",
    "    error_info = {\n",
    "        'error_type': type(error).__name__,\n",
    "        'error_message': str(error),\n",
    "        'agent_name': agent_name,\n",
    "        'query': query[:100] + '...' if len(query) > 100 else query,\n",
    "        'timestamp': datetime.now().isoformat(),\n",
    "        'suggested_actions': []\n",
    "    }\n",
    "    \n",
    "    # Add specific suggestions based on error type\n",
    "    if 'timeout' in str(error).lower():\n",
