    "def agent_error_handler(func):\n",
    "    \"\"\"\n",
    "    Decorator for handling agent execution errors.\n",
    "    \"\"\"\n",
    "    @wraps(func)\n",
    "    def wrapper(*args, **kwargs):\n",
    "        try:\n",
    "            return func(*args, **kwargs)\n",
    "        except TimeoutError as e:\n",
    "            logger.error(f\"Timeout in {func.__name__}: {e}\")\n",
    "            return {\n",
    "                'success': False,\n",
    "                'error': 'Request timed out. Please try again with a simpler query.',\n",
    "                'error_type': 'timeout',\n",
    "                'suggestions': [\n",
    "                    'Try a more specific query',\n",
    "                    'Check your internet connection',\n",
    "                    'Retry the request'\n",
    "                ]\n",
    "            }\n",
    "        except ValidationError as e:\n",
    "            logger.error(f\"Validation error in {func.__name__}: {e}\")\n",
    "            return {\n",
    "                'success': False,\n",
    "                'error': str(e),\n",
    "                'error_type': 'validation',\n",
    "                'suggestions': [\n",
    "                    'Check your input format',\n",
    "                    'Refer to the examples provided',\n",
    "                    'Contact support if the issue persists'\n",
    "                ]\n",
    "            }\n",
    "        except Exception as e:\n",
    "            logger.error(f\"Unexpected error in {func.__name__}: {e}\")\n",
    "            return {\n",
    "                'success': False,\n",
    "                'error': f'An unexpected error occurred: {str(e)}',\n",
    "                'error_type': 'general',\n",
    "                'suggestions': [\n",
    "                    'Try rephrasing your request',\n",
    "                    'Check if all required information is provided',\n",
    "                    'Contact support if the problem continues'\n",
    "                ]\n",
    "            }\n",
    "    return wrapper\n",
    "\n",
    "@agent_error_handler\n",
    "def safe_agent_query(agent, query: str, state=None, timeout: int = 120) -> Dict[str, Any]:\n",
    "    \"\"\"\n",
    "    Safely execute agent query with comprehensive error handling.\n",
    "    \n",
    "    Args:\n",
    "        agent: The agent to query\n",
    "        query: Query string\n",
    "        state: State object\n",
    "        timeout: Timeout in seconds\n",
    "    \n",
    "    Returns:\n",
    "        Dictionary with response or error information\n",
    "    \"\"\"\n",
    "    if not agent:\n",
    "        raise AgentError(\"Agent is not available or not properly initialized\")\n",
    "    \n",
    "    if not query or not isinstance(query, str) or len(query.strip()) == 0:\n",
    "        raise ValidationError(\"Query must be a non-empty string\")\n",
    "    \n",
    "    if state is None:\n",
    "        state = State()\n",
    "    \n",
    "    start_time = datetime.now()\n",
    "    \n",
    "    try:\n",
    "        response = agent.run(query=query.strip(), state=state)\n",
    "        \n",
    "        end_time = datetime.now()\n",
    "        duration = (end_time - start_time).total_seconds()\n",
    "        \n",
    "        return {\n",
    "            'success': True,\n",
    "            'response': response,\n",
    "            'agent_name': getattr(agent, 'name', 'Unknown'),\n",
    "            'duration_seconds': duration,\n",
    "            'timestamp': end_time.isoformat(),\n",
    "            'state_keys': list(state.data.keys()) if hasattr(state, 'data') else []\n",
    "        }\n",
    "    \n",
    "    except Exception as e:\n",
    "        # Re-raise to be handled by decorator\n",
    "        raise e\n",
    "\n",
