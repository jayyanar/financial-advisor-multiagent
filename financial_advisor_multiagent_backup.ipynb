    "                'Use the validation functions to check inputs',\n",
    "                'Review the input requirements'\n",
    "            ]\n",
    "        })\n",
    "    \n",
    "    except AgentError as e:\n",
    "        logger.error(f\"Agent error: {e}\")\n",
    "        response_data.update({\n",
    "            'error': str(e),\n",
    "            'error_type': 'agent_unavailable',\n",
    "            'suggestions': [\n",
    "                'Check if agents are properly initialized',\n",
    "                'Verify model configuration',\n",
    "                'Restart the notebook session',\n",
    "                'Check the agent health status'\n",
    "            ]\n",
    "        })\n",
    "    \n",
    "    except Exception as e:\n",
    "        logger.error(f\"Unexpected error in enhanced_query_agent: {e}\")\n",
    "        response_data.update({\n",
    "            'error': f\"Unexpected error: {str(e)}\",\n",
    "            'error_type': 'unexpected',\n",
    "            'suggestions': [\n",
    "                'Try restarting the notebook kernel',\n",
    "                'Check for any missing dependencies',\n",
    "                'Review the error logs for more details',\n",
    "                'Contact support with the error details'\n",
    "            ]\n",
    "        })\n",
    "    \n",
    "    return response_data\n",
    "\n",
    "def enhanced_print_response(title: str, response_data: Dict[str, Any], \n",
    "                          max_length: int = 1000, show_metadata: bool = False,\n",
    "                          show_errors: bool = True, show_suggestions: bool = True):\n",
    "    \"\"\"\n",
    "    Enhanced response printing with comprehensive error display and formatting.\n",
    "    \n",
    "    Args:\n",
    "        title: Title for the response\n",
    "        response_data: Response data from enhanced_query_agent\n",
    "        max_length: Maximum length before truncation (0 for no truncation)\n",
    "        show_metadata: Whether to display response metadata\n",
    "        show_errors: Whether to display error information\n",
    "        show_suggestions: Whether to display error suggestions\n",
    "    \"\"\"\n",
    "    print(f\"\\n{title}\")\n",
    "    print(\"=\" * len(title))\n",
    "    \n",
    "    if response_data is None:\n",
    "        print(\"❌ No response data available\")\n",
    "        return\n",
    "    \n",
    "    # Show metadata if requested\n",
    "    if show_metadata:\n",
    "        print(f\"Agent: {response_data.get('agent_name', 'Unknown')}\")\n",
    "        print(f\"Duration: {response_data.get('duration_seconds', 0):.2f}s\")\n",
    "        print(f\"Timestamp: {response_data.get('timestamp', 'Unknown')}\")\n",
    "        if response_data.get('state_keys'):\n",
    "            print(f\"State Keys: {', '.join(response_data['state_keys'])}\")\n",
    "        if response_data.get('fallback_used'):\n",
    "            print(\"⚠️ Fallback response used due to error\")\n",
    "        print(\"-\" * 40)\n",
    "    \n",
    "    # Handle success/error status\n",
    "    if not response_data.get('success', False):\n",
    "        if show_errors and response_data.get('error'):\n",
    "            error_type = response_data.get('error_type', 'unknown')\n",
    "            error_icon = {\n",
    "                'validation': '🔧',\n",
    "                'timeout': '⏰',\n",
    "                'api': '🔑',\n",
    "                'network': '🌐',\n",
    "                'agent': '🤖',\n",
    "                'agent_unavailable': '❌',\n",
    "                'unexpected': '⚠️'\n",
    "            }.get(error_type, '❌')\n",
    "            \n",
    "            print(f\"{error_icon} Error ({error_type}): {response_data['error']}\")\n",
    "            \n",
    "            if show_suggestions and response_data.get('suggestions'):\n",
    "                print(\"\\n💡 Suggestions:\")\n",
    "                for suggestion in response_data['suggestions']:\n",
    "                    print(f\"   • {suggestion}\")\n",
    "        \n",
    "        # If there's a fallback response, show it\n",
    "        if response_data.get('fallback_used') and response_data.get('response'):\n",
    "            print(\"\\n🔄 Fallback Response:\")\n",
    "            print(\"-\" * 20)\n",
    "    \n",
    "    # Display the response content\n",
    "    response_text = response_data.get('response', '')\n",
    "    if response_text:\n",
    "        if max_length > 0 and len(str(response_text)) > max_length:\n",
    "            print(str(response_text)[:max_length] + \"...\")\n",
    "            print(f\"\\n[Response truncated - showing first {max_length} characters of {len(str(response_text))} total]\")\n",
    "            print(\"💡 Tip: Use max_length=0 to see full response\")\n",
    "        else:\n",
    "            print(str(response_text))\n",
    "    elif response_data.get('success'):\n",
    "        print(\"⚠️ Empty response received (but query was successful)\")\n",
    "    else:\n",
    "        print(\"❌ No response available\")\n",
    "\n",
