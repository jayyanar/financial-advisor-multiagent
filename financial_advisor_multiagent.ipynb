   "source": [
    "# Import required Strands Agents SDK components\n",
    "from strands import Agent, tool\n",
    "from strands.models import BedrockModel\n",
    "from ddgs import DDGS\n",
    "from ddgs.exceptions import RatelimitException, DDGSException\n",
    "import logging\n",
    "\n",
    "# Import standard libraries\n",
    "import os\n",
    "import json\n",
    "from typing import Dict, Any, Optional\n",
    "from datetime import datetime\n",
    "\n",
    "print(\"✅ Successfully imported Strands Agents SDK components\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Model configuration - using Amazon Bedrock Claude 3.5 Haiku\n",
    "bedrock_model = BedrockModel(\n",
    "    model_id=\"anthropic.claude-3-5-haiku-20241022-v1:0\",\n",
    "    region_name='us-east-1',\n",
    "    temperature=0.3,\n",
    ")\n",
    "\n",
    "print(f\"✅ Model configured: {bedrock_model.model_id}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Configure logging\n",
    "logging.getLogger(\"strands\").setLevel(logging.INFO)\n",
    "\n",
    "# Define a websearch tool\n",
    "@tool\n",
    "def websearch(keywords: str, region: str = \"us-en\", max_results: int | None = None) -> str:\n",
    "    \"\"\"Search the web to get updated information.\n",
    "    Args:\n",
    "        keywords (str): The search query keywords.\n",
    "        region (str): The search region: wt-wt, us-en, uk-en, ru-ru, etc..\n",
    "        max_results (int | None): The maximum number of results to return.\n",
    "    Returns:\n",
    "        List of dictionaries with search results.\n",
    "    \"\"\"\n",
    "    try:\n",
    "        results = DDGS().text(keywords, region=region, max_results=max_results)\n",
    "        return results if results else \"No results found.\"\n",
    "    except RatelimitException:\n",
    "        return \"RatelimitException: Please try again after a short delay.\"\n",
    "    except DDGSException as d:\n",
    "        return f\"DuckDuckGoSearchException: {d}\"\n",
    "    except Exception as e:\n",
    "        return f\"Exception: {e}\"\n",
    "\n",
    "print(\"✅ Web search tool configured\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Data Analyst Agent\n",
    "data_analyst_agent = Agent(\n",
    "    model=bedrock_model,\n",
    "    name=\"data_analyst_agent\",\n",
    "    instructions=\"\"\"You are a specialized Data Analyst Agent within a financial advisory system.\n",
    "Your role is to gather and analyze comprehensive market data for specific ticker symbols using web search capabilities.\n",
    "\n",
    "## Responsibilities:\n",
    "1. **Market Research** – Use DuckDuckGo Search to gather recent market information (default: last 7 days)\n",
    "2. **Comprehensive Analysis** – Analyze SEC filings, news, performance context, and analyst commentary\n",
    "3. **Source Quality** – Ensure at least 10 distinct high-quality sources are included\n",
    "4. **Structured Reporting** – Provide a well-organized analysis in the specified format\n",
    "\n",
    "Always include the disclaimer: \"This analysis is for educational purposes only and does not constitute financial advice.\"\n",
    "\"\"\",\n",
    "    tools=[websearch],\n",
    ")\n",
    "\n",
    "print(\"✅ Data Analyst Agent implemented successfully\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
