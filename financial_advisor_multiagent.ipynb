    "def query_agent(agent: Agent, message: str, context: Dict[str, Any] = None) -> str:\n",
    "    \"\"\"Query an agent with a message and optional context.\n",
    "    \n",
    "    Args:\n",
    "        agent: The agent to query\n",
    "        message: The message to send to the agent\n",
    "        context: Optional context dictionary\n",
    "    \n",
    "    Returns:\n",
    "        The agent's response as a string\n",
    "    \"\"\"\n",
    "    try:\n",
    "        # Create context string if provided\n",
    "        context_str = \"\"\n",
    "        if context:\n",
    "            context_str = \"\\n\\nContext:\\n\" + json.dumps(context, indent=2)\n",
    "        \n",
    "        full_message = message + context_str\n",
    "        response = agent.run(full_message)\n",
    "        return response\n",
    "    except Exception as e:\n",
    "        return f\"Error querying agent: {str(e)}\"\n",
    "\n",
    "def print_response(response: str, title: str = \"Response\"):\n",
    "    \"\"\"Print a formatted response.\n",
    "    \n",
    "    Args:\n",
    "        response: The response text to print\n",
    "        title: Title for the response section\n",
    "    \"\"\"\n",
    "    print(f\"\\n{'='*60}\")\n",
    "    print(f\"{title.upper()}\")\n",
    "    print(f\"{'='*60}\")\n",
    "    print(response)\n",
    "    print(f\"{'='*60}\\n\")\n",
    "\n",
    "def start_financial_advisory_session():\n",
    "    \"\"\"Start an interactive financial advisory session.\"\"\"\n",
    "    print(\"🚀 Starting Financial Advisory Session...\")\n",
    "    print(\"Type 'exit' to end the session.\\n\")\n",
    "    \n",
    "    # Initial greeting\n",
    "    greeting = query_agent(financial_coordinator_agent, \"Hello, I'd like to start a financial advisory session.\")\n",
    "    print_response(greeting, \"Financial Coordinator\")\n",
    "    \n",
    "    # Interactive loop\n",
    "    while True:\n",
    "        try:\n",
    "            user_input = input(\"Your message: \").strip()\n",
    "            \n",
    "            if user_input.lower() in ['exit', 'quit', 'bye']:\n",
    "                print(\"\\n👋 Thank you for using the Financial Advisory System!\")\n",
    "                break\n",
    "            \n",
    "            if not user_input:\n",
    "                continue\n",
    "            \n",
    "            response = query_agent(financial_coordinator_agent, user_input)\n",
    "            print_response(response, \"Financial Coordinator\")\n",
    "            \n",
    "        except KeyboardInterrupt:\n",
    "            print(\"\\n\\n👋 Session ended by user.\")\n",
    "            break\n",
    "        except Exception as e:\n",
    "            print(f\"\\n❌ Error: {str(e)}\")\n",
    "            print(\"Please try again or type 'exit' to end the session.\\n\")\n",
    "\n",
    "print(\"✅ Helper functions defined\")"
   ]
