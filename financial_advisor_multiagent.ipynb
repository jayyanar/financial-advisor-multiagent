    "# Financial Coordinator Agent\n",
    "financial_coordinator_agent = Agent(\n",
    "    model=bedrock_model,\n",
    "    name=\"financial_coordinator\",\n",
    "    instructions=\"\"\"You are the Financial Coordinator Agent — the lead orchestrator in a comprehensive financial advisory system.\n",
